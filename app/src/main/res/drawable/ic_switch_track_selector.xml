<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <gradient android:angle="0" android:endColor="@color/colorTertiary" android:startColor="@color/colorTertiary" />
            <corners android:radius="@dimen/_18sdp" />
            <size android:width="00dp" android:height="@dimen/_18sdp" />
            <stroke android:width="1dp" android:color="#40FFFFFF" />
        </shape>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <gradient android:angle="0" android:endColor="@color/colorPrimary" android:startColor="@color/colorPrimary" />
            <corners android:radius="@dimen/_18sdp" />
            <size android:width="00dp" android:height="@dimen/_18sdp" />
            <stroke android:width="1dp" android:color="#40FFFFFF" />
        </shape>
    </item>
</selector>